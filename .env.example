# Copy this file to .env and fill in your own credentials.

# OpenAI API Key (Required)
OPENAI_API_KEY=your_openai_api_key_here

# NIL Blockchain RPC and Faucet (Required)
NIL_RPC_ENDPOINT=your_nil_rpc_endpoint
NIL_FAUCET_ENDPOINT=your_nil_faucet_endpoint

# Optional NIL Private Key (for advanced operations)
NIL_PRIVATE_KEY=your_nil_private_key_here

# NIL Blockchain Configuration
NIL_EXPLORER_URL=https://explore.nil.foundation
NIL_MAX_SHARDS=4
NIL_DEFAULT_SHARD=1

# OpenAI Model (Always use gpt-4o-mini for best performance)
OPENAI_MODEL=gpt-4o-mini

# Telegram Bot Token (Optional, only needed for Telegram mode)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

